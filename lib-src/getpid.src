// getpid.src

computer = get_shell.host_computer

getpid = function() 
	procs = computer.show_procs
	list = procs.split(char(10))[1:]
	
	for item in list
		parsedItem = item.split(" ")
		if parsedItem[4] == program_path.split("/")[-1] then
			return parsedItem[1]
		end if
	end for

	return null
end function
